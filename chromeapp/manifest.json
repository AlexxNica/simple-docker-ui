{
  "name": "Simple Docker UI",
  "short_name": "Docker UI",
  "description": "Simple Docker UI for Chrome is an unofficial developer tool for monitoring and managing your local Docker containers.",
  "version": "0.0.0.5",
  "manifest_version": 5,
  "minimum_chrome_version": "33.0.1715.0",
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["launch.js"]
    }
  },
    "permissions": [
    "http://*/*", "https://*/*",
      "https://www.google-analytics.com/",
      "storage"
  ],
  "sockets": {
    "tcp": {
      "connect": "*:*"
    }
  }
}
